{
    "name": "zerkxubas/esewa-laravel",
    "description": "This package makes the integration of Esewa payment gateway easier.",
    "type": "library",
    "license": "MIT",
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.42",
        "phpunit/phpunit": "^9.6.15",
        "rector/rector": "^0.18.13"
    },
    "autoload": {
        "psr-4": {
            "Zerkxubas\\EsewaLaravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Subash Sharma",
            "email": "zerkxubas@gmail.com",
            "homepage": "https://zerkbro.com"
        }
    ],
    "keywords": [
        "php",
        "laravel",
        "esewa",
        "payment",
        "nepal"
    ],
    "require": {
        "php": ">=8.0",
        "ext-curl": "*",
        "ext-simplexml": "*"
    },
    "extra": {
        "laravel": {
          "providers": [
            "Zerkxubas\\EsewaLaravel\\Laravel\\EsewaServiceProvider"
          ]
        }
    }
}
